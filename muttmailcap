# PDF-Files
application/octetstream; ~/.mutt/mutt_bgrun.sh /usr/bin/evince '%s'; nametemplate=%s.pdf; test=test "$DISPLAY" != ""; description=Portable
application/octet-stream; ~/.mutt/mutt_bgrun.sh /usr/bin/evince '%s'; nametemplate=%s.pdf; test=test "$DISPLAY" != ""; description=Portable
application/pdf; ~/.mutt/mutt_bgrun.sh /usr/bin/evince '%s'; test=test "$DISPLAY" != ""; description=Portable

# PS-Files
application/postscript; ~/.mutt/mutt_bgrun.sh /usr/bin/gv '%s'; test=test -n "$DISPLAY"; description=postscript

# Images
image/*; ~/.mutt/mutt_bgrun.sh geeqie '%s'; test=test -n "$DISPLAY"

# office
