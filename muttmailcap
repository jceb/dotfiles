text/html; ~/.config/mutt/mutt_bgrun.sh x-www-browser '%s'; nametemplate=%s.html; test=test -n "$DISPLAY"; description=Portable
# text/html; w3m -I %{charset} -T text/html -dump %s; nametemplate=%s.html; copiousoutput
text/html; elinks -dump -dump-charset utf-8 %s; nametemplate=%s.html; copiousoutput

# PDF-Files
application/octetstream; ~/.config/mutt/mutt_bgrun.sh /usr/bin/okular '%s'; nametemplate=%s.pdf; test=test "$DISPLAY" != ""; description=Portable
application/octet-stream; ~/.config/mutt/mutt_bgrun.sh /usr/bin/okular '%s'; nametemplate=%s.pdf; test=test "$DISPLAY" != ""; description=Portable
application/pdf; ~/.config/mutt/mutt_bgrun.sh /usr/bin/okular '%s'; test=test "$DISPLAY" != ""; description=Portable

# PS-Files
application/postscript; ~/.config/mutt/mutt_bgrun.sh /usr/bin/okular '%s'; test=test -n "$DISPLAY"; description=postscript

# Images
image/*; ~/.config/mutt/mutt_bgrun.sh geeqie '%s'; test=test -n "$DISPLAY"

