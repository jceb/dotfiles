! Retrieve current keymap: xmodmap -pk
! Retrieve modifier keymap: xmodmap -pm

! Required to allow key repeat for caps lock key
clear Lock
! set caps lock to control
keycode 66 = Control_L

! swap Esc and ` key
! keycode 49 = Escape
! disable Esc key in preparation for UHK
! keycode 9 = grave asciitilde grave asciitilde dead_tilde asciitilde dead_tilde
! keycode 9 = Escape

! use print key as right windows key
keycode 107 = Super_R
keycode 218 = Super_R
keycode 135 = Super_R
